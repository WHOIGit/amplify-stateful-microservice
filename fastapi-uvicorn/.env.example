# S3 Configuration (for local S3-compatible storage like MinIO)
S3_ENDPOINT_URL=http://localhost:9000
S3_BUCKET=ifcb-features
S3_ACCESS_KEY=minioadmin
S3_SECRET_KEY=minioadmin
S3_USE_SSL=false

# Storage paths (S3 prefixes)
S3_DATASETS_PREFIX=datasets
S3_RESULTS_PREFIX=results

# Multipart upload settings
MULTIPART_PART_SIZE_MB=5
MULTIPART_URL_TTL_SECONDS=7200

# Job settings
MAX_CONCURRENT_JOBS=3
JOB_TIMEOUT_SECONDS=3600

# Output shard settings
MASKS_SHARD_SIZE_MB=256
MASKS_PER_SHARD_ESTIMATE=10000

# Processing settings
BATCH_SIZE=32
